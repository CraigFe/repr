type test_record1 = {
  alpha: string ;
  beta: int64 list ;
  gamma: unit }[@@deriving repr]
include
  struct
    let test_record1_t =
      Repr.Type.sealr
        (Repr.Type.(|+)
           (Repr.Type.(|+)
              (Repr.Type.(|+)
                 (Repr.Type.record "test_record1"
                    (fun alpha ->
                       fun beta -> fun gamma -> { alpha; beta; gamma }))
                 (Repr.Type.field "alpha" Repr.Type.string (fun t -> t.alpha)))
              (Repr.Type.field "beta" (Repr.Type.list Repr.Type.int64)
                 (fun t -> t.beta)))
